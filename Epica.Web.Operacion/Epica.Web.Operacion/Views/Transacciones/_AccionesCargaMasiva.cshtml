@model Epica.Web.Operacion.Models.Request.CargaBachRequest
<div class="dropdown dropdown-inline">
    <a href="javascript:;" class="btn btn-sm btn-icon btn-light btn-active-light-primary me-2" data-bs-toggle="dropdown">
        <i class="bi bi-three-dots"></i>
    </a>
    <ul class="dropdown-menu menu menu-sub menu-sub-dropdown menu-column menu-rounded menu-gray-600 menu-state-bg-light-primary fw-semibold fs-7 w-150px py-4">
        <li class="menu-item px-3">
            <a class="menu-link px-3" onclick="EliminarTransaccionBatch(@Model.Id)" data-id="@Model.Id" title="Borrar Transacción">
                <i class="bi bi-trash"></i>&nbsp;
                Borrar
            </a>
        </li>
        <li class="menu-item px-3">
            <a class="menu-link px-3" onclick="EditarTransaccionBatch(@Model.Id)" data-id="@Model.Id" title="Editar Transacción">
                <i class="bi bi-pencil"></i>&nbsp;
                Editar
            </a>
        </li>
    </ul>
</div>
