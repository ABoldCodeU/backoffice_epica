@model Epica.Web.Operacion.Models.Response.TransaccionSPEIINResponse

<div class="dropdown dropdown-inline">

    <a href="javascript:;" class="btn btn-sm btn-icon btn-light btn-active-light-primary me-2" data-bs-toggle="dropdown">
        <i class="bi bi-three-dots"></i>
    </a>
    <ul class="dropdown-menu menu menu-sub menu-sub-dropdown menu-column menu-rounded menu-gray-600 menu-state-bg-light-primary fw-semibold fs-7 w-150px py-4">
        <li class="menu-item px-3">
            <a data-id="@Model.ClaveRastreo" onclick="AcreditarAbono('@Model.ClaveRastreo')" title="Acreditar Abono" class="btnEditar menu-link px-3">
                <i class="bi bi-check"></i>&nbsp;
                Acreditar
            </a>
        </li>
        <li class="menu-item px-3">
            <a data-id="@Model.ClaveRastreo" onclick="RechazarAbono('@Model.ClaveRastreo')" title="Rechazar Abono" class="btnEditar menu-link px-3">
                <i class="bi bi-x"></i>&nbsp;
                Rechazar
            </a>
        </li>
    </ul> 
</div>